{
  "name": "@nullstyle/urand",
  "version": "0.2.0",
  "exports": "./src/mod.ts",
  "tasks": {
    "build": "zig build-exe -target wasm32-freestanding -O ReleaseSmall -rdynamic -fno-entry src/prng.zig -femit-bin=prng.wasm && deno run -A scripts/embed.ts",
    "test": "deno task build && deno test"
  },
  "imports": {
    "@std/assert": "jsr:@std/assert@^0.226.0",
    "@std/encoding": "jsr:@std/encoding@^1.0.10"
  },
  "publish": {
    "include": ["src/", "README.md", "LICENSE"],
    "exclude": ["**/*.test.ts", "examples/", "tests/"]
  }
}
